<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fxmlControllers.PositionController">
   <children>
      <TableView fx:id="positionTableView" layoutX="14.0" layoutY="14.0" prefHeight="310.0" prefWidth="378.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="68.0" text="ИД" />
          <TableColumn fx:id="nameColumn" prefWidth="309.0" text="Название должности" />
        </columns>
      </TableView>
      <Button layoutX="479.0" layoutY="69.0" mnemonicParsing="false" onAction="#addPosition" prefHeight="54.0" prefWidth="93.0" text="Добавить должность" textAlignment="CENTER" wrapText="true" />
      <Button layoutX="479.0" layoutY="200.0" mnemonicParsing="false" onAction="#deletePosition" prefHeight="54.0" prefWidth="93.0" text="Удалить должность" textAlignment="CENTER" wrapText="true" />
      <TextField fx:id="positionTextField" layoutX="423.0" layoutY="30.0" promptText="Название должности" />
      <Button layoutX="479.0" layoutY="134.0" mnemonicParsing="false" onAction="#editPosition" prefHeight="54.0" prefWidth="93.0" text="Изменить" />
   </children>
</AnchorPane>
